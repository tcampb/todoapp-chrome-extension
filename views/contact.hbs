   <header>
        <div class="profile" style="padding-top: 5px;">
         <a href="/profile" data-profile><img src={{img}} width="40px" height="40px"></a>
        </div>
        {{> header}}
    </header>
   <div class="container" data-container>
    <div class="console">
    <a href="/" data-back><i class="fa fa-arrow-left blue" aria-hidden="true"></i></a>
    </div>


<div class="container ui" id="primaryForm">
    <div class="ui cards">
    {{#if contact}}
    {{#each contact}}
        <div class="card">
            <div class="content">
                <div class="header">{{name}}</div>
                </div>
        <div class="content">
            <p><i class="mail icon"></i>       {{email}}</p>
        </div>
            <div class="extra content">
                <div class="ui bottom attached button" data-contact-id={{id}}>
                    <i class="remove user icon"></i>
                    Remove Contact
                </div>
            </div>
            </div>
    {{/each}}
    {{else}}
    <h3 style="color: rgba(0,0,0,0.44)">You have no contacts yet.</h3>
    {{/if}}
    </div>
    </div>
        </body>
    <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui-calendar/0.0.8/calendar.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.2.10/dist/semantic.min.js"></script>
    <script src="javascripts/task.js"></script> 
