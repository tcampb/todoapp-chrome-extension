{
  "manifest_version": 2,
  "name": "Do it extention",
  "version": "1.0",
  "description": "Create and manage tasks on task lists and connect these to their google calendar.",

//   "icons": {
//     "128": "icon128.png",
//     "48": "icon48.png",
//     "16": "icon16.png"
//   },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens Get It Done Extension"
    }
  }

}